<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xs:schema version="1.1.0"
           xmlns="http://Santander.com/Schemas/Integrations/CommonDataModel/CommonTypes"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           attributeFormDefault="qualified"
           elementFormDefault="qualified"
           targetNamespace="http://Santander.com/Schemas/Integrations/CommonDataModel/CommonTypes">
    <!--Worker and PECI -->
    <xs:simpleType name="YearType">
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="9999"/>
        </xs:restriction>
    </xs:simpleType>
    <!--Worker and PECI -->
    <xs:element name="Employee" type="EmployeeElement_DataType"/>
    <xs:complexType name="EmployeeElement_DataType">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="Summary" type="SummaryType"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="Related_Person" type="Related_PersonType"/>

        </xs:sequence>
        <xs:attribute name="Sequence" type="xs:nonNegativeInteger" use="optional"/>
    </xs:complexType>
    <xs:complexType name="SummaryType">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="WID" type="StringType"></xs:element>
            <xs:element maxOccurs="1" minOccurs="1" name="Employee_ID" type="StringType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Prehire_ID" type="StringType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Name" type="StringType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Updated_From_Override" type="xs:dateTime"></xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Workday_Account" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Related_PersonType">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="Related_Person_ID" type="TextFieldType"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="Related_Person_Type" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Dependent_ID" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Beneficiary_ID" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Emergency_Contact_ID" type="TextFieldType"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="Legal_Name" type="Related_Person-Legal_NameType"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="Citizenship" type="Related_Person-CitizenshipType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Relationship_Type" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Gender" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Birth_Date" type="DateFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Date_of_Death" type="DateFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Country_of_Birth" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Region_of_Birth" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="City_of_Birth" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Nationality" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Tobacco_Use" type="BooleanFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Full_Time_Student" type="BooleanFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Is_Disabled" type="BooleanFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Dependent" type="BooleanFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Beneficiary" type="BooleanFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Emergency_Contact" type="BooleanFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Is_Dependent_for_Payroll_Purposes" type="BooleanFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Inactive_Date" type="DateFieldType">
                <xs:annotation>
                    <xs:documentation>The inactive date for the Dependent. This is a date type field.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Relative" type="BooleanFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Relative_Type" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Lives_with_Worker" type="TextFieldType">
                <xs:annotation>
                    <xs:documentation>The dependent lives with the employee at his/her primary home location.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Has_Health_Insurance" type="TextFieldType">
                <xs:annotation>
                    <xs:documentation>The dependent has health insurance.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Allowed_for_Tax_Deduction" type="TextFieldType">
                <xs:annotation>
                    <xs:documentation>The dependent can be included for Tax Deduction purpose.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Annual_Income_Effective_Date" type="DateFieldType">
                <xs:annotation>
                    <xs:documentation>The dependent's annual income effective date.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Annual_Income_Currency" type="TextFieldType">
                <xs:annotation>
                    <xs:documentation>The dependent's annual income currency.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Annual_Income_Amount" type="NumericFieldType">
                <xs:annotation>
                    <xs:documentation>The dependent's annual income amount.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="Disability_Data"
                        type="Related_Person-Disability_DataType">
                <xs:annotation>
                    <xs:documentation>The dependent's disability data.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Great_Disability_Grade" type="StringType">
                <xs:annotation>
                    <xs:documentation>This is calculated field required by the interface
                        ESP_PEOPLESOFT_EMPLOYEE_TO_DependientesYFamiliares_BATCH
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Marital_Status" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Title" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Related_Person_Effective_Date" type="DateFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="First_Name_in_Local_Script" type="StringType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Middle_Name_in_Local_Script" type="StringType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Last_Name_in_Local_Script" type="StringType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Secondary_Last_Name_in_Local_Script" type="StringType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="First_Name_in_Local_Script_2" type="StringType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Last_Name_in_Local_Script_2" type="StringType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Formatted_Phone_Number" type="StringType"/>
            <xs:element name="Operation" type="StringType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="Education" type="StringType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="Family_Situation" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="Family_Record" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="ESP_Situacion_Familiar" type="StringType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="Family_Employee_ID" type="TextFieldType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="Disability_Percentage" type="TextFieldType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
        <xs:attribute name="isAdded" type="xs:boolean"/>
        <xs:attribute name="isDeleted" type="xs:boolean"/>
        <xs:attribute name="isUpdated" type="xs:boolean"/>
        <xs:attribute name="IID" type="xs:string"/>
    </xs:complexType>
    <xs:complexType name="TextFieldType">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="isAdded" type="xs:boolean"/>
                <xs:attribute name="isDeleted" type="xs:boolean"/>
                <xs:attribute name="priorValue" type="xs:string"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="Related_Person-Legal_NameType">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="Country" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="First_Name" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Middle_Name" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Last_Name" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Secondary_Last_Name" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="First_Name_Local" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Middle_Name_Local" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Last_Name_Local" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Secondary_Last_Name_Local" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="First_Name_Local_2" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Middle_Name_Local_2" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Last_Name_Local_2" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Secondary_Last_Name_Local_2" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Tertiary_Last_Name" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Hereditary" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Salutation" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Social_Suffix" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Title" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Full_Name_Malaysia_Singapore" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="General_Display_Name" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Name_in_Local_Script" type="TextFieldType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Formatted_Name" type="TextFieldType"></xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Reporting_Name" type="TextFieldType"></xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Full_Legal_Name" type="TextFieldType"></xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Marital_Status" type="TextFieldType"/>
        </xs:sequence>
        <xs:attribute name="isAdded" type="xs:boolean"/>
        <xs:attribute name="isDeleted" type="xs:boolean"/>
        <xs:attribute name="isUpdated" type="xs:boolean"/>
        <xs:attribute name="IID" type="xs:string"/>
    </xs:complexType>
    <xs:complexType name="Related_Person-CitizenshipType">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="Citizenship_Status_ID" type="TextFieldType"/>
        </xs:sequence>
        <xs:attribute name="isAdded" type="xs:boolean"/>
        <xs:attribute name="isDeleted" type="xs:boolean"/>
        <xs:attribute name="isUpdated" type="xs:boolean"/>
        <xs:attribute name="IID" type="xs:string"/>
    </xs:complexType>
    <xs:complexType name="DateFieldType">
        <xs:simpleContent>
            <xs:extension base="xs:date">
                <xs:attribute name="isAdded" type="xs:boolean"/>
                <xs:attribute name="isDeleted" type="xs:boolean"/>
                <xs:attribute name="priorValue" type="xs:string"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="BooleanFieldType">
        <xs:simpleContent>
            <xs:extension base="xs:boolean">
                <xs:attribute name="isAdded" type="xs:boolean"/>
                <xs:attribute name="isDeleted" type="xs:boolean"/>
                <xs:attribute name="priorValue" type="xs:string"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="NumericFieldType">
        <xs:simpleContent>
            <xs:extension base="RestrictedDecimal">
                <xs:attribute name="isAdded" type="xs:boolean"/>
                <xs:attribute name="isDeleted" type="xs:boolean"/>
                <xs:attribute name="priorValue" type="xs:string"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:simpleType name="RestrictedDecimal">
        <xs:restriction base="xs:decimal">
            <xs:totalDigits value="26"/>
            <xs:fractionDigits value="6"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="Related_Person-Disability_DataType">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="Disability" type="TextFieldType">
                <xs:annotation>
                    <xs:documentation>The dependent's disability description.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Status_Date" type="DateFieldType">
                <xs:annotation>
                    <xs:documentation>The dependent's disability status date.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Date_Known" type="DateFieldType">
                <xs:annotation>
                    <xs:documentation>The dependent's disability date known.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="End_Date" type="DateFieldType">
                <xs:annotation>
                    <xs:documentation>The dependent's disability end date.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Grade" type="TextFieldType">
                <xs:annotation>
                    <xs:documentation>The dependent's disability grade.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="isAdded" type="xs:boolean"/>
        <xs:attribute name="isDeleted" type="xs:boolean"/>
        <xs:attribute name="isUpdated" type="xs:boolean"/>
        <xs:attribute name="IID" type="xs:string"/>
    </xs:complexType>
    <xs:complexType name="StringType">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="PriorValue" type="xs:string"/>
                <xs:attribute name="truncated" type="xs:boolean"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    </xs:schema>